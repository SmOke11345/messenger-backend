generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// TODO: создать таблицу Friend и связать ее с полем friends в таблице Users, это нужно чтобы потом выводить друзей пользователя по id;

model Users {
  id          Int     @id @default(autoincrement())
  name        String
  lastname    String?
  password    String
  profile_img String?
  email       String
}
